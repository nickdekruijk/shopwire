{
    "name": "nickdekruijk/shopwire",
    "description": "A simple, easy to implement shopping cart and checkout package for Laravel using Livewire.",
    "keywords": ["webshop", "shopwire", "laravel", "shop", "cart", "checkout", "livewire", "ecommerce", "shopping", "shoppingcart"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/nickdekruijk/shopwire/issues",
        "source": "https://github.com/nickdekruijk/shopwire"
    },
    "authors": [{
        "name": "Nick de Kruijk",
        "email": "git@nickdekruijk.nl"
    }],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.48.29|^11.0|^12.0",
        "livewire/livewire": "^3.0",
        "mollie/laravel-mollie": "^3.0",
        "monarobase/country-list": "^3.0",
        "stevebauman/location": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "NickDeKruijk\\Shopwire\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NickDeKruijk\\Shopwire\\ShopwireServiceProvider"
            ],
            "aliases": {
                "Shopwire": "NickDeKruijk\\Shopwire\\ShopwireFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
